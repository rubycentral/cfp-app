.draggable-session-card{data: ps.ps_data.merge({ draggable_sessions_target: "session" })}
  .track
    %span=truncate(ps.track_name, length: 18)
  .session-card-body
    %p.card-title= truncate(ps.title, length: 40, separator: ' ')
    %p.card-speakers= truncate(ps.display_speakers, length: 40, separator: ' ')
