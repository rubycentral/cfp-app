$('table#organizer-rooms #room_<%= room.id %>').remove();
reloadSessionsTable(<%=raw sessions.rows.to_json %>);

<% if has_missing_requirements?(room.event) %>
  $('#session-prereqs').removeClass('hidden')
  $('#add-session').addClass('disabled')

  document.getElementById('missing-prereq-messages').innerHTML =
    '<%=j unmet_requirements(room.event) %>';
<% end %>
