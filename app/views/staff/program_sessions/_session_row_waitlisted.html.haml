%tr{ data: { 'session-id' => program_session.id } }
  %td= link_to(program_session.title,
      event_staff_program_session_path(program_session.event, program_session))
  %td= program_session.speakers.map { |speaker| link_to speaker.name, event_staff_program_speaker_path(program_session.event, speaker) }.join(", ").html_safe
  %td= program_session.session_format_name
  %td= program_session.track_name
  %td= program_session.state_label
  %td= program_session.confirmed_status
  %td= link_to 'Promote', promote_event_staff_program_session_path(program_session.event, program_session), method: :patch, data: { confirm: "Are you sure you want to promote #{program_session.title}?" }, class: 'btn btn-warning btn-xs'
  %td= program_session.confirmation_notes_link
